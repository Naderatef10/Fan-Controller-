#include "motor.h"
#include "gpio.h"


void DcMotor_Init(void){

GPIO_setupPinDirection( MOTOR_PORT,INPUT1_PIN,PIN_INPUT);
GPIO_setupPinDirection( MOTOR_PORT,INPUT1_PIN,PIN_INPUT);

GPIO_writePin(MOTOR_PORT,INPUT1_PIN,LOGIC_LOW);
GPIO_writePin(MOTOR_PORT,INPUT2_PIN,LOGIC_LOW);






}
